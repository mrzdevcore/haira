// AI block test with integer operations

// Return the answer to life
ai get_answer() -> int {
    Return 42, the answer to life, the universe, and everything.
}

// Add two numbers
ai add(a: int, b: int) -> int {
    Return the sum of a and b.
}

// Check if number is positive
ai is_positive(x: int) -> bool {
    Return true if x is greater than zero, false otherwise.
}

// Main program
answer = get_answer()
print(answer)

sum = add(10, 32)
print(sum)

positive = is_positive(42)
print(positive)

negative = is_positive(5)
print(negative)
